msgid ""
"「では、どうぞ、\n"
"　ごゆっくり。"
msgstr ""
"“Thank you.\n"
" Have a good rest.”"

msgctxt "// INN_MSG07"
msgid ""
"「ほかの部屋に\n"
"　なさいますか？"
msgstr ""
"“Would you like\n"
" a different room?”"

